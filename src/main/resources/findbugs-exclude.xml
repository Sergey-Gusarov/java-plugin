<FindBugsFilter>
    <Match>
        <!-- deprecated. Используйте исключения, специфичные для каждой из библиотек. Или делайте игнор через аннотации в коде.  -->
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,SE_BAD_FIELD,ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD,UMAC_UNCALLABLE_METHOD_OF_ANONYMOUS_CLASS,DM_DEFAULT_ENCODING,URF_UNREAD_FIELD,UWF_UNWRITTEN_FIELD,
            DRE_DECLARED_RUNTIME_EXCEPTION,USBR_UNNECESSARY_STORE_BEFORE_RETURN,PRMC_POSSIBLY_REDUNDANT_METHOD_CALLS, EXS_EXCEPTION_SOFTENING_NO_CHECKED, EXS_EXCEPTION_SOFTENING_NO_CONSTRAINTS,
            NAB_NEEDLESS_BOOLEAN_CONSTANT_CONVERSION,SPP_EQUALS_ON_ENUM,CE_CLASS_ENVY,CC_CYCLOMATIC_COMPLEXITY,OCP_OVERLY_CONCRETE_PARAMETER,BAS_BLOATED_ASSIGNMENT_SCOPE"/>
    </Match>
    <Match>
        <!-- exclude mapstruct-->
        <Source name="~..*\.mapping\..*?MapperImpl.*"/>
    </Match>
</FindBugsFilter>
